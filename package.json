{
  "name": "typescript-study",
  "private": true,
  "version": "0.0.1",
  "description": "typescript study",
  "scripts": {
    "build": "cross-env NODE_ENV=production rimraf dist/* && tsc",
    "watch": "cross-env NODE_ENV=development rimraf dist/* && tsc -w",
    "lint": "eslint --ext .js,.ts --ignore-path .gitignore .",
    "lintfix": "eslint --fix --ext .js,.ts --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn precommit"
    }
  },
  "author": {
    "name": "hisasann",
    "email": "hisasann25@gmail.com",
    "url": "https://github.com/hisasann"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hisasann/typescript-study.git"
  },
  "keywords": [
    "nuxt-ts",
    "typescript"
  ],
  "bugs": {
    "url": "https://github.com/hisasann/typescript-study/issues"
  },
  "dependencies": {
    "cross-env": "^5.2.0",
    "rimraf": "^2.6.3"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^0.0.1",
    "@types/babel-core": "6.25.6",
    "@types/eslint": "^4.16.6",
    "@types/eslint-plugin-prettier": "2.2.0",
    "@types/jest": "^24.0.9",
    "@types/prettier": "1.16.1",
    "@typescript-eslint/eslint-plugin": "^1.4.2",
    "@typescript-eslint/parser": "^1.4.2",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "eslint": "^5.0.1",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-standard": ">=12.0.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": ">=2.14.0",
    "eslint-plugin-jest": ">=21.24.1",
    "eslint-plugin-node": ">=7.0.1",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-promise": ">=4.0.1",
    "eslint-plugin-standard": ">=4.0.0",
    "husky": "^1.3.1",
    "jest": "^24.1.0",
    "prettier": "1.16.4",
    "typescript": "^3.3.3333"
  }
}
